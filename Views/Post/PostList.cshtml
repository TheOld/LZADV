                            <button class="btn btn-link dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                Categoria
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                <li><a href="#" class="filter" data-filter="all" id="filter-all">Todos</a></li>
                                <li><a href="#" class="filter" data-filter=".customs-law">Direito aduaneiro</a></li>
                                <li><a href="#" class="filter" data-filter=".tax-law">Direito tributário</a></li>
                                <li><a href="#" class="filter" data-filter=".labor-law">Direito do trabalho e previdenciário</a></li>
